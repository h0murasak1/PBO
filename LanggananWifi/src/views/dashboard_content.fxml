<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.DashboardContentController">
    <TableView fx:id="dashboardTable" layoutX="10" layoutY="10" prefWidth="500" prefHeight="400">
        <columns>
            <TableColumn fx:id="colName" text="Name" prefWidth="100"/>
            <TableColumn fx:id="colPlanName" text="Plan Name" prefWidth="100"/>
            <TableColumn fx:id="colPrice" text="Price" prefWidth="100"/>
            <TableColumn fx:id="colStartDate" text="Start Date" prefWidth="100"/>
            <TableColumn fx:id="colEndDate" text="End Date" prefWidth="100"/>
        </columns>
    </TableView>
</AnchorPane>
